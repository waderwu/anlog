import requests

def replay():
    url = "http://{host}{uri}"
    headers = {}
    headers["Content-Type"] = "application/x-www-form-urlencoded"
    tmp = {headers}
    for key in tmp:
        headers[key] = tmp[key]

    data = """{data}"""
    r = requests.{method}(url, data=data, headers=headers, timeout=5)

    return r.text


if __name__ == "__main__":
    print(replay())